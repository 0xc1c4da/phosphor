        "italic"
      ]
    },
    "generic_inserted": {
      "fg": "#00d787"
    },
    "generic_strong": {
      "attrs": [
        "bold"
      ]
    },
    "generic_subheading": {
      "fg": "#777777"
    },
    "background": {
      "bg": "#373737"
    }
  }
}
